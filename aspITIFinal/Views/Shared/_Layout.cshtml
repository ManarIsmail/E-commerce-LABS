    <!-- ======= Modal ======= -->
    <div class="modal fade" id="searchModel" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content modelsearch">
                <div class="modal-body">
                    <form asp-action="searchproduct" asp-controller="Home" method="post">
                        <input class="inputsearch" type="text" name="nameproduct" id="nameproduct" placeholder="Enter Brand" value="">
                        <input class="btnsearch" type="submit" value="Search">
                    </form>
                </div>
            </div>
        </div>
    </div><!-- End Modal -->
    <!-- ======= Main ======= -->
    <main id="main">
        @RenderBody()
    </main><!-- End main -->
